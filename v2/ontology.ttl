@prefix : <http://schema.oceanproteinportal.org/v2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://schema.oceanproteinportal.org/v2/> .

<http://schema.oceanproteinportal.org/v2/> rdf:type owl:Ontology ;
                                            owl:versionIRI :opp.owl .

#################################################################
#    Annotation properties
#################################################################

###  http://schema.oceanproteinportal.org/v2/conceptualModel
:conceptualModel rdf:type owl:AnnotationProperty ;
                 rdfs:comment "A link to a visual drawing of the conceputal model for a certain class" ;
                 rdfs:label "conceptualModel" ;
                 rdfs:range xsd:anyURI ;
                 rdfs:domain owl:Thing .


###  http://schema.oceanproteinportal.org/v2/describedinConceptualModelOf
:describedinConceptualModelOf rdf:type owl:AnnotationProperty ;
                              rdfs:comment "The conceptual model the describes this class can be found as an annotation of the following target class." ;
                              rdfs:label "describedinConceptualModelOf" ;
                              rdfs:range owl:Thing ;
                              rdfs:domain owl:Thing .


###  http://schema.oceanproteinportal.org/v2/profileObservation
:profileObservation rdf:type owl:AnnotationProperty ;
                    rdfs:range <http://www.w3.org/ns/ssn/Property> .


###  http://www.w3.org/2002/07/owl#sameAs
owl:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://schema.oceanproteinportal.org/v2/aboutIdentifier
:aboutIdentifier rdf:type owl:ObjectProperty ;
                 owl:inverseOf :identifierMetadata ;
                 rdfs:domain :IdentifierMetadata ;
                 rdfs:range :Identifier ;
                 rdfs:comment "This metadata is about the specified Identifier" ;
                 rdfs:label "aboutIdentifier" .


###  http://schema.oceanproteinportal.org/v2/annotatesIdentifier
:annotatesIdentifier rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     owl:inverseOf :identifierMetadata ;
                     rdfs:domain :IdentifierMetadata ;
                     rdfs:range :Identifier ;
                     rdfs:comment "The identifer for which this information object annotates" ;
                     rdfs:label "annotatesIdentifier" .


###  http://schema.oceanproteinportal.org/v2/conformsToProfile
:conformsToProfile rdf:type owl:ObjectProperty ;
                   rdfs:domain <http://schema.org/Dataset> ;
                   rdfs:range :DatasetProfile ;
                   rdfs:comment "The dataset conforms to the Dataset Profile. Meaning to presents the profile's expected observations." ;
                   rdfs:label "conformsToProfile" .


###  http://schema.oceanproteinportal.org/v2/definesIdentifierScheme
:definesIdentifierScheme rdf:type owl:ObjectProperty ;
                         rdfs:domain owl:Thing ;
                         rdfs:range :IdentifierScheme ;
                         rdfs:comment "The IdentifierScheme defined by some Thing. In the case fo the OPP, a single Dataset defines an IdentifierScheme for its local identifiers." ;
                         rdfs:label "definesIdentifierScheme" .


###  http://schema.oceanproteinportal.org/v2/describedByIdentification
:describedByIdentification rdf:type owl:ObjectProperty ;
                           owl:inverseOf :describesAminoAcid ;
                           rdfs:domain :AminoAcidChain ;
                           rdfs:range :AminoAcidIdentification ;
                           rdfs:comment "The  identification that describes this amino acid" ;
                           rdfs:label "describedByIdentification" .


###  http://schema.oceanproteinportal.org/v2/describesAminoAcid
:describesAminoAcid rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :AminoAcidIdentification ;
                    rdfs:range :AminoAcidChain ;
                    rdfs:comment "The AminoAcidChain being described by this information." ;
                    rdfs:label "describesAminoAcid" .


###  http://schema.oceanproteinportal.org/v2/describesPeptide
:describesPeptide rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :describesAminoAcid ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :PeptideIdentification ;
                  rdfs:range :Peptide ;
                  rdfs:comment "The actual Peptide being described by this information." ;
                  rdfs:label "describesPeptide" .


###  http://schema.oceanproteinportal.org/v2/describesProtein
:describesProtein rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :describesAminoAcid ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :ProteinIdentification ;
                  rdfs:range :Protein ;
                  rdfs:comment "The actual Protein being described by this Protien information." ;
                  rdfs:label "describesProtein" .


###  http://schema.oceanproteinportal.org/v2/hasAgentRole
:hasAgentRole rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range :AgentRole ;
              rdfs:comment "An AgentRole in which an Agent performs some Role on this particular Thing." ;
              rdfs:label "hasAgentRole" .


###  http://schema.oceanproteinportal.org/v2/hasFilterSize
:hasFilterSize rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://www.w3.org/ns/ssn/systems/hasSystemCapability> ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain <http://www.w3.org/ns/sosa/Sampler> ;
               rdfs:range :FilterSize ;
               rdfs:comment "The size of the filter(s) being used during the sampling process. This could be a single value using schema:value property or a range of sizes using schema:minValue & schema:maxValue." ;
               rdfs:label "hasFilterSize" .


###  http://schema.oceanproteinportal.org/v2/identificationBasedOnScore
:identificationBasedOnScore rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :identificationScore ;
                            rdfs:domain :AminoAcidIdentification ;
                            rdfs:range :IdentificationScore ;
                            rdfs:comment "The score used to determine an amino acid's identification" ;
                            rdfs:label "identificationBasedOnScore" .


###  http://schema.oceanproteinportal.org/v2/identificationScore
:identificationScore rdf:type owl:ObjectProperty ;
                     rdfs:domain :AminoAcidIdentification ;
                     rdfs:range :IdentificationScore ;
                     rdfs:comment "The score within a certain system for identifying an amino acid." ;
                     rdfs:label "identificationScore" .


###  http://schema.oceanproteinportal.org/v2/identifier
:identifier rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :isIdentifierOf ;
            rdfs:domain owl:Thing ;
            rdfs:range :Identifier ;
            rdfs:comment "An Identifier with a known IdentifierScheme for some Thing." ;
            rdfs:label "identifier" .


###  http://schema.oceanproteinportal.org/v2/identifierMetadata
:identifierMetadata rdf:type owl:ObjectProperty ;
                    rdfs:domain :Identifier ;
                    rdfs:range :IdentifierMetadata ;
                    rdfs:comment "An information record that annotated or further describes this Identifier." ;
                    rdfs:label "identifierMetadata" .


###  http://schema.oceanproteinportal.org/v2/identifierScheme
:identifierScheme rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isIdentifierSchemeFor ;
                  rdf:type owl:FunctionalProperty ,
                           owl:IrreflexiveProperty ;
                  rdfs:domain :Identifier ;
                  rdfs:range :IdentifierScheme ;
                  rdfs:comment "The scheme in which this Identifier has a unique value." ;
                  rdfs:label "identifierScheme" .


###  http://schema.oceanproteinportal.org/v2/inRole
:inRole rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        owl:inverseOf :roleFor ;
        rdf:type owl:IrreflexiveProperty ;
        rdfs:domain :AgentRole ;
        rdfs:range :Role ;
        rdfs:comment "Role being performed by some Agent" ;
        rdfs:label "inRole" .


###  http://schema.oceanproteinportal.org/v2/inVicinityOfStation
:inVicinityOfStation rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
                     rdfs:range :Station ;
                     rdfs:comment "The one, named or mutally agreed upon location at which Sampling occurred. This usually means a single coordinate at which sampling occurs depsite any variance in the location of the sensor at the time of measurement. This Station helps to normalize the intent of sampling at a certain location for cross-comparison between datasets." ;
                     rdfs:label "inVicinityOfStation" .


###  http://schema.oceanproteinportal.org/v2/isAgentRoleFor
:isAgentRoleFor rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :AgentRole ;
                rdfs:range owl:Thing ;
                rdfs:comment "The Thing on/in which an Agent performs some Role." ;
                rdfs:label "isAgentRoleFor" .


###  http://schema.oceanproteinportal.org/v2/isIdentifierOf
:isIdentifierOf rdf:type owl:ObjectProperty ;
                rdfs:domain :Identifier ;
                rdfs:range owl:Thing ;
                rdfs:comment "The thing being identified" ;
                rdfs:label "isIdentifierOf" .


###  http://schema.oceanproteinportal.org/v2/isIdentifierSchemeFor
:isIdentifierSchemeFor rdf:type owl:ObjectProperty ,
                                owl:InverseFunctionalProperty ,
                                owl:IrreflexiveProperty ;
                       rdfs:domain :IdentifierScheme ;
                       rdfs:range :Identifier ;
                       rdfs:comment "An Identifier that bleongs within this scheme." ;
                       rdfs:label "isIdentifierSchemeFor" .


###  http://schema.oceanproteinportal.org/v2/keggOrthology
:keggOrthology rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :identifierMetadata ;
               rdfs:domain :KeggOrthologyIdentifier ;
               rdfs:range :KeggOrthology ;
               rdfs:label "keggOrthology" .


###  http://schema.oceanproteinportal.org/v2/keggPathway
:keggPathway rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :keggPathwayForOrthology ;
             rdfs:domain :KeggOrthology ;
             rdfs:range :KeggPathway ;
             rdfs:comment "A Pathway for some Kegg Orthology entry." ;
             rdfs:label "keggPathway" .


###  http://schema.oceanproteinportal.org/v2/keggPathwayForOrthology
:keggPathwayForOrthology rdf:type owl:ObjectProperty ;
                         rdfs:domain :KeggPathway ;
                         rdfs:range :KeggOrthology ;
                         rdfs:comment "The orthology for which this pathway belongs" ;
                         rdfs:label "keggPathwayForOrthology" .


###  http://schema.oceanproteinportal.org/v2/ncbiTaxonName
:ncbiTaxonName rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :identifierMetadata ;
               rdfs:domain :NCBITaxonIdentifier ;
               rdfs:range :NCBITaxonName ;
               rdfs:comment "The NCBI Identifier for which this name describes." ;
               rdfs:label "ncbiTaxonName" .


###  http://schema.oceanproteinportal.org/v2/observationInSample
:observationInSample rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://www.w3.org/ns/sosa/hasFeatureOfInterest> ;
                     rdfs:domain :AminoAcidSampleObservation ;
                     rdfs:range <http://www.w3.org/ns/sosa/Sample> ;
                     rdfs:comment "An observation made about a single Sample for some identified amino acid." ;
                     rdfs:label "observationInSample" .


###  http://schema.oceanproteinportal.org/v2/observedPeptide
:observedPeptide rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://www.w3.org/ns/sosa/Sample> ;
                 rdfs:range :PeptideIdentification ;
                 rdfs:comment "The PeptideIdentification being asserted by the observer of some Sample." ;
                 rdfs:label "observedPeptide" .


###  http://schema.oceanproteinportal.org/v2/performedAgentRole
:performedAgentRole rdf:type owl:ObjectProperty ;
                    owl:inverseOf :performedBy ;
                    rdf:type owl:IrreflexiveProperty ;
                    rdfs:domain :Agent ;
                    rdfs:range :AgentRole ;
                    rdfs:comment "The AgentRole which was performed by this Agent" ;
                    rdfs:label "performedAgentRole" .


###  http://schema.oceanproteinportal.org/v2/performedAtOrganization
:performedAtOrganization rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdf:type owl:IrreflexiveProperty ;
                         rdfs:domain :AgentRole ;
                         rdfs:range <http://schema.org/Organization> ;
                         rdfs:comment "The Organization at which the acting Agent performed the Role" ;
                         rdfs:label "performedAtOrganization" .


###  http://schema.oceanproteinportal.org/v2/performedBy
:performedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdf:type owl:IrreflexiveProperty ;
             rdfs:domain :AgentRole ;
             rdfs:range :Agent ;
             rdfs:comment "The Agent acting in some Role." ;
             rdfs:label "performedBy" .


###  http://schema.oceanproteinportal.org/v2/performedOnBehalfOf
:performedOnBehalfOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdfs:domain :AgentRole ;
                     rdfs:range :Agent ;
                     rdfs:comment "The Agent for which the performing Agent is acting in a certain Role." ;
                     rdfs:label "performedOnBehalfOf" .


###  http://schema.oceanproteinportal.org/v2/profileObservation
:profileObservation rdf:type owl:ObjectProperty ;
                    rdfs:domain :DatasetProfile .


###  http://schema.oceanproteinportal.org/v2/providesRole
:providesRole rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range :Role ;
              rdfs:comment "The existence of some Thing provides the ability for some Agent to perform this Role." ;
              rdfs:label "providesRole" .


###  http://schema.oceanproteinportal.org/v2/roleFor
:roleFor rdf:type owl:ObjectProperty ,
                  owl:IrreflexiveProperty ;
         rdfs:domain :Role ;
         rdfs:range :AgentRole ;
         rdfs:comment "An AgentRole performing this specific Role" ;
         rdfs:label "roleFor" .


###  http://schema.oceanproteinportal.org/v2/sampleAminoAcidProperty
:sampleAminoAcidProperty rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://www.w3.org/ns/sosa/observedProperty> ;
                         rdf:type owl:IrreflexiveProperty ;
                         rdfs:domain :AminoAcidIdentification ;
                         rdfs:range :AminoAcidSampleObservation ;
                         rdfs:comment "An observation made from a single Sample about an identified amino acid" ;
                         rdfs:label "sampleAminoAcidProperty" .


###  http://schema.oceanproteinportal.org/v2/samplePeptideProperty
:samplePeptideProperty rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :sampleAminoAcidProperty ;
                       rdfs:domain :PeptideIdentification ;
                       rdfs:range :PeptideSampleObservation ;
                       rdfs:comment "An observation made from a single Sample about an identified peptide" ;
                       rdfs:label "samplePeptideProperty" .


###  http://schema.oceanproteinportal.org/v2/sampleProteinProperty
:sampleProteinProperty rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :sampleAminoAcidProperty ;
                       rdfs:domain :ProteinIdentification ;
                       rdfs:range :ProteinSampleObservation ;
                       rdfs:comment "An observation made from a single Sample about an identified protein" ;
                       rdfs:label "sampleProteinProperty" .


###  http://schema.oceanproteinportal.org/v2/selectedBasedOnPeptideIdentification
:selectedBasedOnPeptideIdentification rdf:type owl:ObjectProperty ;
                                      rdfs:subPropertyOf :sugggestedByPeptideIdentification ;
                                      owl:inverseOf :selectedProteinIdentification ;
                                      rdfs:domain :BestProteinIdentification ;
                                      rdfs:range :PeptideIdentification ;
                                      rdfs:comment "This Best Identified Protein was selected by the data'sinterpreter based on  the peptide identification." ;
                                      rdfs:label "selectedBasedOnPeptideIdentification" .


###  http://schema.oceanproteinportal.org/v2/selectedProteinIdentification
:selectedProteinIdentification rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :suggestsProteinIdentification ;
                               rdfs:domain :PeptideIdentification ;
                               rdfs:range :BestProteinIdentification ;
                               rdfs:comment "The selected protein identification is the Protein that the interpreter of the data felt was the best protein identificationout of all possible proteins it may be." ;
                               rdfs:label "selectedProteinIdentification" .


###  http://schema.oceanproteinportal.org/v2/storesResultsForSample
:storesResultsForSample rdf:type owl:ObjectProperty ;
                        rdfs:domain <http://schema.org/Dataset> ;
                        rdfs:range <http://www.w3.org/ns/sosa/Sample> ;
                        rdfs:comment "A sample for which this Dataset stores observations results for." ;
                        rdfs:label "storesResultsForSample" .


###  http://schema.oceanproteinportal.org/v2/suggestsProteinIdentification
:suggestsProteinIdentification rdf:type owl:ObjectProperty ;
                               owl:inverseOf :suggestsProteinIdentification ;
                               rdfs:domain :ProteinIdentification ;
                               rdfs:range :PeptideIdentification ;
                               rdfs:comment "A peptide identification suggests this identified protein as possibly being contained within the related Sample." ;
                               rdfs:label "suggestsProteinIdentification" .


###  http://schema.oceanproteinportal.org/v2/sugggestedByPeptideIdentification
:sugggestedByPeptideIdentification rdf:type owl:ObjectProperty ;
                                   rdfs:comment "A ProteinIdentification was made based on this PeptideIdentification. The peptide suggests that the identified Protein may be contained in the Sample." ;
                                   rdfs:label "suggestedByPeptideIdentification" .


###  http://www.w3.org/ns/sosa/hasFeatureOfInterest
<http://www.w3.org/ns/sosa/hasFeatureOfInterest> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/sosa/observedProperty
<http://www.w3.org/ns/sosa/observedProperty> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/ssn/systems/hasSystemCapability
<http://www.w3.org/ns/ssn/systems/hasSystemCapability> rdf:type owl:ObjectProperty ;
                                                       rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://schema.oceanproteinportal.org/v2/absoluteUnits
:absoluteUnits rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf <http://schema.org/value> ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :AminoAcidIdentification ;
               rdfs:range xsd:float ;
               rdfs:comment "absolute units of a peptide or protein identification measured in fmol/liter" ;
               rdfs:label "absoluteUnits" .


###  http://schema.oceanproteinportal.org/v2/aminoAcidName
:aminoAcidName rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :AminoAcidIdentification ;
               rdfs:range xsd:string ;
               rdfs:comment "The name given to the amino acid being identified by some identifying agent." ;
               rdfs:label "aminoAcidName" .


###  http://schema.oceanproteinportal.org/v2/aminoAcidSequence
:aminoAcidSequence rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :AminoAcidChain ;
                   rdfs:range xsd:token ;
                   rdfs:comment "The amino acid chain, written as a string of characters, that identifies a specific amino acid." ;
                   rdfs:label "aminoAcidSequence" .


###  http://schema.oceanproteinportal.org/v2/bestSEQUESTDCnScore
:bestSEQUESTDCnScore rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :PeptideIdentification ;
                     rdfs:range xsd:float ;
                     rdfs:comment "The best SEQUEST DCn score" ;
                     rdfs:label "bestSEQUESTDCnScore" .


###  http://schema.oceanproteinportal.org/v2/bestSEQUESTXCorrScore
:bestSEQUESTXCorrScore rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain :PeptideIdentification ;
                       rdfs:range xsd:float ;
                       rdfs:comment "The best SEQUEST XCorr score" ;
                       rdfs:label "bestSEQUESTXCorrScore" .


###  http://schema.oceanproteinportal.org/v2/cruiseID
:cruiseID rdf:type owl:DatatypeProperty ;
          rdfs:domain :Cruise ;
          rdfs:range xsd:string ;
          rdfs:comment "Some label for a certain cruise on which an OPP Dataset was collected." ;
          rdfs:label "cruiseID" .


###  http://schema.oceanproteinportal.org/v2/depth
:depth rdf:type owl:DatatypeProperty ;
       rdfs:subPropertyOf owl:topDataProperty ;
       rdf:type owl:FunctionalProperty ;
       rdfs:domain <http://www.w3.org/ns/sosa/FeatureOfInterest> ;
       rdfs:range xsd:float ;
       rdfs:comment "The depth in meters where some sample was collected." ;
       rdfs:label "depth" ;
       rdfs:seeAlso <ssn:hasProperty> .


###  http://schema.oceanproteinportal.org/v2/identificationProbability
:identificationProbability rdf:type owl:DatatypeProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain :AminoAcidIdentification ;
                           rdfs:range xsd:float ;
                           rdfs:comment "Thee probability of the correctness/confidence of the identification" ;
                           rdfs:label "identificationProbability" .


###  http://schema.oceanproteinportal.org/v2/identifierValue
:identifierValue rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Identifier ;
                 rdfs:range xsd:token ;
                 rdfs:comment "The unique value in some IdentifierScheme" ;
                 rdfs:label "identifierValue" .


###  http://schema.oceanproteinportal.org/v2/keggName
:keggName rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf <http://schema.org/name> ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :KeggOrthology
                                      :KeggPathway
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:comment "The name of some Entry in the Kegg Database. This could be thee name of an Orthology or Pathway, etc." ;
          rdfs:label "keggName" .


###  http://schema.oceanproteinportal.org/v2/keggOrthologyDefinition
:keggOrthologyDefinition rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf <http://schema.org/description> ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain :KeggOrthology ;
                         rdfs:range xsd:string ;
                         rdfs:comment """The Kegg Orthology Definition

Example: \"nitrate reductase / nitrite oxidoreductase, alpha subunit [EC:1.7.5.1 1.7.99.-]\" via `https://www.genome.jp/dbget-bin/www_bget?K00370`""" ;
                         rdfs:label "keggOrthologyDefinition" .


###  http://schema.oceanproteinportal.org/v2/maximumFilterSize
:maximumFilterSize rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf <http://schema.org/maxValue> ;
                   rdfs:domain :FilterSize ;
                   rdfs:range xsd:float ;
                   rdfs:comment "The maximum size of the filter used during sampling. Measured in microns." ;
                   rdfs:label "maximumFilterSize" .


###  http://schema.oceanproteinportal.org/v2/medianRetentionTime
:medianRetentionTime rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :PeptideIdentification ;
                     rdfs:range xsd:float ;
                     rdfs:label "medianRetentionTime" .


###  http://schema.oceanproteinportal.org/v2/minimumFilterSize
:minimumFilterSize rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf <http://schema.org/minValue> ;
                   rdfs:domain :FilterSize ;
                   rdfs:range xsd:float ;
                   rdfs:comment "The minimum size of a filter used during sampling. Mesaured in microns." ;
                   rdfs:label "minimumFilterSize" .


###  http://schema.oceanproteinportal.org/v2/molecularWeight
:molecularWeight rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :AminoAcidIdentification ;
                 rdfs:range xsd:float ;
                 rdfs:comment "A protein's molecular weight measured in kiloDaltons" ;
                 rdfs:label "molecularWeight" .


###  http://schema.oceanproteinportal.org/v2/peptideSequence
:peptideSequence rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :aminoAcidSequence ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :Peptide ;
                 rdfs:range xsd:token ;
                 rdfs:comment "The amino acid chain, written as a string of characters, that identifies a specific Peptide." ;
                 rdfs:label "peptideSequence" .


###  http://schema.oceanproteinportal.org/v2/plus2HSpectraCount
:plus2HSpectraCount rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :PeptideSpectralCountSum ;
                    rdfs:range xsd:float ;
                    rdfs:label "plus2HSpectraCount" .


###  http://schema.oceanproteinportal.org/v2/plus3HSpectraCount
:plus3HSpectraCount rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :PeptideSpectralCountSum ;
                    rdfs:range xsd:float ;
                    rdfs:label "plus3HSpectraCount" .


###  http://schema.oceanproteinportal.org/v2/plus4HSpectraCount
:plus4HSpectraCount rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :PeptideSpectralCountSum ;
                    rdfs:range xsd:float ;
                    rdfs:label "plus4HSpectraCount" .


###  http://schema.oceanproteinportal.org/v2/proteinName
:proteinName rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :aminoAcidName ;
             rdf:type owl:FunctionalProperty ;
             rdfs:domain :ProteinIdentification ;
             rdfs:range xsd:string ;
             rdfs:comment "The name given to the protein being identified by some identifying agent." ;
             rdfs:label "proteinName" .


###  http://schema.oceanproteinportal.org/v2/proteinSequence
:proteinSequence rdf:type owl:DatatypeProperty ;

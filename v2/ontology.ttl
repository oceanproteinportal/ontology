###  http://schema.oceanproteinportal.org/v2/Agent
:Agent rdf:type owl:Class ;
       :describedinConceptualModelOf :AgentRole ;
       rdfs:comment "An abstract class for specifying a person, organization or software entity that performs some action acting in some predefined role." ;
       rdfs:label "Agent" .


###  http://schema.oceanproteinportal.org/v2/AgentIdentifier
:AgentIdentifier rdf:type owl:Class ;
                 rdfs:subClassOf :Identifier ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :isIdentifierOf ;
                                   owl:someValuesFrom :Agent
                                 ] ;
                 rdfs:comment "An identifier for an Agent" ;
                 rdfs:label "AgentIdentifier" .


###  http://schema.oceanproteinportal.org/v2/AgentRole
:AgentRole rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :inRole ;
                             owl:allValuesFrom :Role
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :performedAtOrganization ;
                             owl:allValuesFrom <http://schema.org/Organization>
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :performedBy ;
                             owl:allValuesFrom :Agent
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :inRole ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Role
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :performedBy ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Agent
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :performedAtOrganization ;
                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass <http://schema.org/Organization>
                           ] ;
           :conceptualModel "https://lucid.app/publicSegments/view/0df85414-42b6-4601-9293-956c39559249/image.png"^^xsd:anyURI ;
           rdfs:comment "An agent performing some Role over a certain period of time (which may be unbounded)." ;
           rdfs:label "AgentRole" .


###  http://schema.oceanproteinportal.org/v2/AminoAcidChain
:AminoAcidChain rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :aminoAcidSequence ;
                                  owl:allValuesFrom xsd:token
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :aminoAcidSequence ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onDataRange xsd:token
                                ] ;
                owl:disjointWith :AminoAcidIdentification ;
                :describedinConceptualModelOf :AminoAcidIdentification ;
                rdfs:comment "An actual amino acid. This is not to contain information that helps identify an amino acid, but represents the amino acid being identified. Any information here, should be shared by all other representations of the amino acid."^^xsd:anyURI ;
                rdfs:label "AminoAcidChain" .


###  http://schema.oceanproteinportal.org/v2/AminoAcidIdentification
:AminoAcidIdentification rdf:type owl:Class ;
                         :conceptualModel "https://lucid.app/publicSegments/view/de62351f-8e38-426f-9b35-dcfb696e8729/image.png"^^xsd:anyURI ;
                         rdfs:comment "An information object for storing data about an amino acid." ;
                         rdfs:label "AminoAcidIdentification" .


###  http://schema.oceanproteinportal.org/v2/AminoAcidSampleObservation
:AminoAcidSampleObservation rdf:type owl:Class ;
                            rdfs:subClassOf <http://schema.org/PropertyValue> ,
                                            <http://www.w3.org/ns/ssn/Property> ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :observationInSample ;
                                              owl:allValuesFrom <http://www.w3.org/ns/sosa/Sample>
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :observationInSample ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass <http://www.w3.org/ns/sosa/Sample>
                                            ] ;
                            :describedinConceptualModelOf :AminoAcidIdentification ;
                            rdfs:comment "A property of some amino acid that was observed in a single Sample." ;
                            rdfs:label "AminoAcidSampleObservation" .


###  http://schema.oceanproteinportal.org/v2/BestProteinIdentification
:BestProteinIdentification rdf:type owl:Class ;
                           rdfs:subClassOf :ProteinIdentification ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :selectedBasedOnPeptideIdentification ;
                                             owl:someValuesFrom :PeptideIdentification
                                           ] ;
                           rdfs:comment "The best/selected identification of a specific Protein based on information in a PeptideIdentification. This means that a PeptideIdentification may have multiple Proteins that it identifies but that one of those proteins has been selected (identifiedBy) as the best identification." ;
                           rdfs:label "BestProteinIdentification" .


###  http://schema.oceanproteinportal.org/v2/BestSEQUESTDCnScore
:BestSEQUESTDCnScore rdf:type owl:Class ;
                     rdfs:subClassOf :IdentificationScore ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :scoreValue ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:float
                                     ] ;
                     rdfs:comment "The best SEQUEST DCn score for identifying an amino acid."^^xsd:anyURI ;
                     rdfs:label "BestSEQUESTDCnScore" .


###  http://schema.oceanproteinportal.org/v2/BestSEQUESTXCorrScore
:BestSEQUESTXCorrScore rdf:type owl:Class ;
                       rdfs:subClassOf :IdentificationScore ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :scoreValue ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:float
                                       ] ;
                       rdfs:comment "The best SEQUEST XCorr score for identifying an amino acid."^^xsd:anyURI ;
                       rdfs:label "BestSEQUESTXCorrScore" .


###  http://schema.oceanproteinportal.org/v2/Cruise
:Cruise rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/ns/sosa/Deployment> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :cruiseID ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        owl:disjointWith :Station ;
        rdfs:comment "A deployment of some seagoing vessel on which one or more scientific objectives are studied." ;
        rdfs:label "Cruise" .


###  http://schema.oceanproteinportal.org/v2/DatasetIdentifierScheme
:DatasetIdentifierScheme rdf:type owl:Class ;
                         rdfs:subClassOf :IdentifierScheme ;
                         rdfs:comment "The identifierScheme for a singular OPP dataset. The dataset itself is the realm in which other identifiers such as Station names, protein names, etc. are unique (but that may not be unqiue acorss other OPP datasets). This helps ensure that identifiers in a Dataset are not automatically assumed to be equivalent to each other." ;
                         rdfs:label "DatasetIdentifierScheme" .


###  http://schema.oceanproteinportal.org/v2/DatasetProfile
:DatasetProfile rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :profileObservation ;
                                  owl:allValuesFrom <http://www.w3.org/ns/ssn/Property>
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :profileObservation ;
                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass <http://www.w3.org/ns/ssn/Property>
                                ] ;
                :describedinConceptualModelOf <http://schema.org/Dataset> ;
                rdfs:comment "A DatasetProfile defines the type of observations to be expected." ;
                rdfs:label "DatasetProfile" .


###  http://schema.oceanproteinportal.org/v2/EnzymeCommissionIdentifier
:EnzymeCommissionIdentifier rdf:type owl:Class ;
                            rdfs:subClassOf :ProteinIdentifier ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :identifierScheme ;
                                              owl:hasValue :IdentifierScheme_EnzymeCommission
                                            ] ;
                            rdfs:comment "An EC identifier" ;
                            rdfs:label "EnzymeCommissionIdentifier" .


###  http://schema.oceanproteinportal.org/v2/FilterSize
:FilterSize rdf:type owl:Class ;
            rdfs:subClassOf <http://schema.org/PropertyValue> ,
                            <http://www.w3.org/ns/ssn/systems/SystemCapability> ;
            :describedinConceptualModelOf <http://www.w3.org/ns/sosa/Sample> ;
            rdfs:comment "The filter size in micons of the sampler." ;
            rdfs:label "FilterSize" .


###  http://schema.oceanproteinportal.org/v2/IdentificationScore
:IdentificationScore rdf:type owl:Class ;
                     rdfs:subClassOf <http://schema.org/PropertyValue> ,
                                     <http://www.w3.org/ns/ssn/Property> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :scoreValue ;
                                       owl:minCardinality "1"^^xsd:nonNegativeInteger
                                     ] ;
                     :describedinConceptualModelOf :AminoAcidIdentification ;
                     rdfs:comment "The numerical score used to identified an Amino Acid using some scoring method." ;
                     rdfs:label "IdentificationScore" .


###  http://schema.oceanproteinportal.org/v2/Identifier
:Identifier rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :identifierScheme ;
                              owl:allValuesFrom :IdentifierScheme
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :identifierScheme ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :IdentifierScheme
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :identifierValue ;
                              owl:allValuesFrom xsd:token
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :identifierValue ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:token
                            ] ;
            :conceptualModel "https://lucid.app/publicSegments/view/18f2a07c-1ddc-40cc-afeb-eff1fee6d7f3/image.png"^^xsd:anyURI ;
            rdfs:comment "A unique identifier attributeds to some thing within some realm or domain or scheme."^^xsd:anyURI ;
            rdfs:label "Identifier" .


###  http://schema.oceanproteinportal.org/v2/IdentifierMetadata
:IdentifierMetadata rdf:type owl:Class ;
                    rdfs:subClassOf <http://schema.org/PropertyValue> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :annotatesIdentifier ;
                                      owl:allValuesFrom :Identifier
                                    ] ;
                    :describedinConceptualModelOf :Identifier ;
                    rdfs:comment "Information about an identifier that helps comprehend, reuse, or contextualize the Thing being identified. This could be a non-unique label or description." ;
                    rdfs:label "IdentifierMetadata" .


###  http://schema.oceanproteinportal.org/v2/IdentifierScheme
:IdentifierScheme rdf:type owl:Class ;
                  :describedinConceptualModelOf :Identifier ;
                  rdfs:comment "A realm or domain in which a specific Identifier has a unqiue value from all the other possible Identifiers within that domain." ;
                  rdfs:label "IdentifierScheme" .


###  http://schema.oceanproteinportal.org/v2/KeggDatabaseEntry
:KeggDatabaseEntry rdf:type owl:Class ;
                   rdfs:subClassOf :IdentifierMetadata ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :annotatesIdentifier ;
                                     owl:someValuesFrom :KeggDatabaseEntryIdentifier
                                   ] ;
                   rdfs:comment "An entry in thee Kegg Database" ;
                   rdfs:label "KeggDatabaseEntry" .


###  http://schema.oceanproteinportal.org/v2/KeggDatabaseEntryIdentifier
:KeggDatabaseEntryIdentifier rdf:type owl:Class ;
                             rdfs:subClassOf :ProteinIdentifier ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :identifierScheme ;
                                               owl:hasValue :IdentifierScheme_Kegg
                                             ] ;
                             rdfs:comment "A Kegg Dataset Entry Identifier" ;
                             rdfs:label "KeggDatabaseEntryIdentifier" .


###  http://schema.oceanproteinportal.org/v2/KeggOrthology
:KeggOrthology rdf:type owl:Class ;
               rdfs:subClassOf :KeggDatabaseEntry ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :annotatesIdentifier ;
                                 owl:someValuesFrom :KeggOrthologyIdentifier
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :keggName ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] ;
               rdfs:comment "A Kegg Orthology entry from the Kegg database" ;
               rdfs:label "KeggOrthology" .


###  http://schema.oceanproteinportal.org/v2/KeggOrthologyIdentifier
:KeggOrthologyIdentifier rdf:type owl:Class ;
                         rdfs:subClassOf :KeggDatabaseEntryIdentifier ;
                         rdfs:comment "A Kegg Orthology Identifier" ;
                         rdfs:label "KeggOrthologyIdentifier" .


###  http://schema.oceanproteinportal.org/v2/KeggPathway
:KeggPathway rdf:type owl:Class ;
             rdfs:subClassOf :KeggDatabaseEntry ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :keggPathwayForOrthology ;
                               owl:someValuesFrom :KeggOrthology
                             ] ;
             rdfs:comment "A Kegg Orthology (KO) entry's pathway." ;
             rdfs:label "KeggPathway" .


###  http://schema.oceanproteinportal.org/v2/NCBITaxonIdentifier
:NCBITaxonIdentifier rdf:type owl:Class ;
                     rdfs:subClassOf :ProteinIdentifier ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :identifierScheme ;
                                       owl:hasValue :IdentifierScheme_NCBIProtein
                                     ] ;
                     rdfs:comment "An NCBI Taxon identifier" ;
                     rdfs:label "NCBITaxonIdentifier" .


###  http://schema.oceanproteinportal.org/v2/NCBITaxonName
:NCBITaxonName rdf:type owl:Class ;
               rdfs:subClassOf :IdentifierMetadata ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :annotatesIdentifier ;
                                 owl:someValuesFrom :NCBITaxonIdentifier
                               ] ;
               rdfs:label "NCBITaxonName" .


###  http://schema.oceanproteinportal.org/v2/OPPGeneratedSampleIdentifier
:OPPGeneratedSampleIdentifier rdf:type owl:Class ;
                              rdfs:subClassOf :SampleIdentifier ;
                              owl:disjointWith :UserDefinedSampleIdentifier ;
                              rdfs:comment "A sample identifier that was generated by the OPP" ;
                              rdfs:label "OPPGeneratedSampleIdentifier" .


###  http://schema.oceanproteinportal.org/v2/OrganizationIdentifier
:OrganizationIdentifier rdf:type owl:Class ;
                        rdfs:subClassOf :AgentIdentifier ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :isIdentifierOf ;
                                          owl:someValuesFrom <http://schema.org/Organization>
                                        ] ;
                        rdfs:comment "An identifier for an Organization" ;
                        rdfs:label "OrganizationIdentifier" .


###  http://schema.oceanproteinportal.org/v2/PFamsDescription
:PFamsDescription rdf:type owl:Class ;
                  rdfs:subClassOf :IdentifierMetadata ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :annotatesIdentifier ;
                                    owl:someValuesFrom :PFamsIdentifier
                                  ] ;
                  rdfs:comment "The PFams description for this Protein" ;
                  rdfs:label "PFamsDescription" .


###  http://schema.oceanproteinportal.org/v2/PFamsIdentifier
:PFamsIdentifier rdf:type owl:Class ;
                 rdfs:subClassOf :ProteinIdentifier ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :identifierScheme ;
                                   owl:hasValue :IdentifierScheme_PFam
                                 ] ;
                 rdfs:comment "A PFams identifier" ;
                 rdfs:label "PFamsIdentifier" .


###  http://schema.oceanproteinportal.org/v2/Peptide
:Peptide rdf:type owl:Class ;
         rdfs:subClassOf :AminoAcidChain ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :peptideSequence ;
                           owl:allValuesFrom xsd:token
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :peptideSequence ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:token
                         ] ;
         rdfs:comment "A peptide identified in a sample." ;
         rdfs:label "Peptide" .


###  http://schema.oceanproteinportal.org/v2/PeptideIdentification
:PeptideIdentification rdf:type owl:Class ;
                       rdfs:subClassOf :AminoAcidIdentification ;
                       rdfs:comment "An information object where a dataset can store observations about an identified peptide in a sample. These observations may vary across many datasets claiming to identify the same peptide, so this entity serves as a way to store these observations separate from other peptide identifiation observations while still allowing these PeptideIdentification objeect to describe the same, singular Peptide." ;
                       rdfs:label "PeptideIdentification" .


###  http://schema.oceanproteinportal.org/v2/PeptideSampleObservation
:PeptideSampleObservation rdf:type owl:Class ;
                          rdfs:subClassOf :AminoAcidSampleObservation ;
                          rdfs:comment "Some property of a peptide identification that was observed in a single sample." ,
                                       "The sum of all spectra count." ;
                          rdfs:label "PeptideSampleObservation" .


###  http://schema.oceanproteinportal.org/v2/PeptideSpectralCountSum
:PeptideSpectralCountSum rdf:type owl:Class ;
                         rdfs:subClassOf :PeptideSampleObservation .


###  http://schema.oceanproteinportal.org/v2/PersonIdentifier
:PersonIdentifier rdf:type owl:Class ;
                  rdfs:subClassOf :AgentIdentifier ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isIdentifierOf ;
                                    owl:someValuesFrom <http://schema.org/Person>
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :identifierScheme ;
                                    owl:hasValue :IdentifierScheme_ORCID
                                  ] ;
                  rdfs:comment "An identifier for a Person" ;
                  rdfs:label "PersonIdentifier" .


###  http://schema.oceanproteinportal.org/v2/Protein
:Protein rdf:type owl:Class ;
         rdfs:subClassOf :AminoAcidChain ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :proteinSequence ;
                           owl:allValuesFrom xsd:token
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :proteinSequence ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:token
                         ] ;
         rdfs:comment "The actual Protein being identified in datasets. This real-world Protein is identified by a single amino acid sequence regardless of variations across  ProteinIdentifications that describe this same, single Protein" ;
         rdfs:label "Protein" .


###  http://schema.oceanproteinportal.org/v2/ProteinIdentification
:ProteinIdentification rdf:type owl:Class ;
                       rdfs:subClassOf :AminoAcidIdentification ;
                       rdfs:comment "A protein identification made based on information of a peptide identification. This entity serves as an information object used within a Dataset to identify and describe an actual Protein. Where a Protein has a single amino acid sequence that will not vary across datasets submitted to the OPP, th ereported protien names, molecular weights, etc may slightly vary across these datasets. ProteinIdentification exists so that individual datasets can report their findings and remain separate from other ProteinIdentification while still describing the exact same Protein with the same amino acid sequence." ;
                       rdfs:label "ProteinIdentification" .


###  http://schema.oceanproteinportal.org/v2/ProteinIdentifier
:ProteinIdentifier rdf:type owl:Class ;
                   rdfs:subClassOf :Identifier ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :isIdentifierOf ;
                                     owl:someValuesFrom :ProteinIdentification
                                   ] ;
                   rdfs:label "ProteinIdentifier" .


###  http://schema.oceanproteinportal.org/v2/ProteinIdentifierScheme
:ProteinIdentifierScheme rdf:type owl:Class ;
                         rdfs:subClassOf :IdentifierScheme ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :isIdentifierSchemeFor ;
                                           owl:someValuesFrom :ProteinIdentifier
                                         ] ;
                         rdfs:comment "A class of identifier schemes used to unqiuely identify proteins." ,
                                      """Kegg Orthology (KO) Entry  => opp:keggEntry (schema:identifier)
KO Definition => opp:keggDefinition (aka schema:description)
KO Pathway => opp:keggPathwayNames (rdf:Bag of KO names)

Note since the order of the pathways (or KeggPathway Index isn't provided we don't use an rdf:Seq""" ;
                         rdfs:label "ProteinIdentifierScheme" .


###  http://schema.oceanproteinportal.org/v2/ProteinSampleObservation
:ProteinSampleObservation rdf:type owl:Class ;
                          rdfs:subClassOf :AminoAcidSampleObservation ;
                          rdfs:comment "Some property of a protein identification that was observed in a single sample." ;
                          rdfs:label "ProteinSampleObservation" .


###  http://schema.oceanproteinportal.org/v2/ProteinTotalSpectralCount
:ProteinTotalSpectralCount rdf:type owl:Class ;
                           rdfs:subClassOf :ProteinSampleObservation .


###  http://schema.oceanproteinportal.org/v2/Role
:Role rdf:type owl:Class ;
      :describedinConceptualModelOf :AgentRole ;
      rdfs:comment "A certain status, or position, played by some Agent that qualifies/contextualizes the actions performed by that Agent." ;
      rdfs:label "Role" .


###  http://schema.oceanproteinportal.org/v2/SampleIdentifier
:SampleIdentifier rdf:type owl:Class ;
                  rdfs:subClassOf :Identifier ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :isIdentifierOf ;
                                    owl:someValuesFrom <http://www.w3.org/ns/sosa/Sample>
                                  ] ;
                  rdfs:comment "An identifier for a sample within a Dataset. The IdentifierScheme for this sample would be provides by the Dataset. See opp:definesIdentifierScheme" ;
                  rdfs:label "SampleIdentifier" .


###  http://schema.oceanproteinportal.org/v2/Station
:Station rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :stationName ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string

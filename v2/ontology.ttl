                      rdfs:subPropertyOf <http://schema.org/value> ;
                      rdf:type owl:FunctionalProperty ;
                      rdfs:range xsd:float ;
                      rdfs:comment "The observed spectral count for a single protein from a single sample" ;
                      rdfs:label "proteinSpectralCount" .



                  rdfs:domain :PeptideIdentification ;
                  rdfs:range xsd:float ;
                  rdfs:comment "The sum of the +2, +3, +4 spectra counts." ;
                  rdfs:label "spectralCountSum" .


###  http://schema.oceanproteinportal.org/v2/startIndex
:startIndex rdf:type owl:DatatypeProperty ;
            rdfs:domain :PeptideIdentification ;
            rdfs:range xsd:integer ;
            rdfs:comment "The start index" ;
            rdfs:label "startIndex" .


###  http://schema.oceanproteinportal.org/v2/stationName
:stationName rdf:type owl:DatatypeProperty ;
             rdfs:domain :Station ;
             rdfs:range xsd:string ;
             rdfs:comment "The name of some Station." ;
             rdfs:label "stationName" .


###  http://schema.oceanproteinportal.org/v2/stopIndex
:stopIndex rdf:type owl:DatatypeProperty ;
           rdfs:domain :PeptideIdentification ;
           rdfs:range xsd:integer ;
           rdfs:comment "The stop index" ;
           rdfs:label "stopIndex" .



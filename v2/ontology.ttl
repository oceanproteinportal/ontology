:totalPrecursorIntensity rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain :PeptideIdentification ;
                         rdfs:range xsd:float ;
                         rdfs:label "totalPrecursorIntensity" .


